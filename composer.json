{
  "name": "juliangut/slim-php-di",
  "description": "Slim Framework PHP-DI container implementation",
  "keywords": [
    "slim",
    "framework",
    "dependency inversion",
    "container"
  ],
  "homepage": "http://github.com/juliangut/slim-php-di",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julián Gutiérrez",
      "email": "juliangut@gmail.com",
      "homepage": "http://juliangut.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "slim/slim": "3.x-dev",
    "php-di/php-di": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "squizlabs/php_codesniffer": "~2",
    "phpmd/phpmd": "~2.2",
    "sebastian/phpcpd": "~2.0",
    "doctrine/cache": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Jgut\\Slim\\PHPDI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jgut\\Slim\\PHPDI\\Tests\\": "tests/PHPDI/"
    }
  }
}
